weight = 10
# Ground Shipping
if weight <= 2:
  cost_ground = 1.5 * weight + 20
elif weight <= 6:
  cost_ground = 3 * weight + 20
elif weight <= 10:
  cost_ground = 4 * weight + 20
else:
  cost_ground = 4.75 * weight + 20

cost_ground_premium = 125.00

# Drone Shipping
if weight <= 2:
  cost_drone = 4.5 * weight
elif weight <= 6:
  cost_drone = 9 * weight
elif weight <= 10:
  cost_drone = 12 * weight
else:
  cost_drone = 14.25 * weight

# Ground Shipping Premium Cost
print("Ground Shipping Premium $", cost_ground_premium)

print("Drone Shipping Cost: $", cost_drone)
print("Ground Shipping Cost: $", cost_ground)
